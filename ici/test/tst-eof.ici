static doit(a)
{
    printf("\n");
    printf("%d\n", eof(a));
    printf("%s\n", string(getchar(a))); /*x*/
    printf("%d\n", eof(a));
    printf("%s\n", string(getchar(a))); /*y*/
    printf("%d\n", eof(a));
    printf("%s\n", string(getchar(a))); /*z*/
    printf("%d\n", eof(a));
    printf("%s\n", string(getchar(a))); /*NULL*/
    printf("%d\n", eof(a));
    close(a);
}

