	MOVL	dcb_tcprcvoosl[EDI], EAX
	MOVL	rcv_pkt.B[EBP], EBX
	MOVZBL	EDX, npb_tpofs.B[EBX]
	MOVL	ECX, npb_count.B[EBX]
	SUBL	ECX, EDX
	MOVL	rcv_tcplen.B[EBP], ECX
	ADDL	EDX, EBX
	MOVL	rcv_tcphdr.B[EBP], EDX
