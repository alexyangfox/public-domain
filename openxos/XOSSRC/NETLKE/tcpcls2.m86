	CMPL	dcb_tcpconbase[EDI], #0.B ;Is this a type 2 open?
	JE	10$.S			;No
	TESTB	tcp_code.B[EDX], #TCPC$FIN ;FIN indicated?
	JNE	16$.S			;Yes - fail	
	CLRL	EAX
	CALL	tcptype2ready		;Yes
	JMP	14$.S

;Here if type 1 open

10$:	CALL	xostcpTimerRmv#		;Stop the connection timer
	MOVL	ESI, dcb_tcpackwake[EDI] ;Stop the ACK timer if its running
	TESTL	ESI, ESI
	JE	12$.S
	CLRL	EAX
	MOVL	dcb_tcpackwake[EDI], EAX
	CALL	knlWakeRemove##
12$:	MOVL	EBX, rcv_pkt.B[EBP]
	MOVL	EDX, rcv_tcphdr.B[EBP]
	TESTB	tcp_code.B[EDX], #TCPC$FIN ;FIN indicated?
	JNE	20$.S			;Yes - fail	
	CLRL	EAX
	CALL	xosnetSndDone##
14$:	MOVZBL	EAX, tcp_offset.B[EDX]	;Get length of TCP header
	SHRL	EAX, #2
	ANDB	AL, #0FCh
	CMPL	rcv_tcplen.B[EBP], EAX
