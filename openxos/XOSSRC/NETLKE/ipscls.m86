	.SBTTL	Initialization routine

	INITSUB	ipsinit

;The command for installing IPSCLS is:
;	LKELOAD IPSCLS

ipsinit:MOVL	EDI, #ipschk		;Install the IPS class
	MOVL	EBX, #ipscls		;Offset of class function dispatch table
	MOVL	EAX, #'IPS'
	CLRL	EDX
	MOVL	ESI, #ipsccb
	CALL	knlNewClass##		;Install the IPS device class
	JC	4$.S			;If error
	CALL	icmpinit#		;Initialize the ICMP routines
	JC	6$.S			;If error
	MOVL	EBX, #ipsoas		;Set up routing once-a-second routine
	CALL	knlSetupOAS##
	MOVL	ECX, #xcodetop#		;OK
	CLRL	EDX
	CLRL	EAX
4$:	MOVL	EBX, #QSTS$DONE
	RET

;Here if error initializing the IC class (IP classe already installed)

6$:	PUSHL	EAX			;Save error code
	MOVL	ESI, #ipsccb		;Remove the IPS class
	CALL	knlRmvClass##
	POPL	EAX			;Restore error code
	STC
	JMP	4$.S

	CODE
.PAGE
	.SBTTL	ipsaddunit - Subroutine to add IPS "unit"

;Subroutine to add IP "unit" - this is a logical unit which implements a
;  link level protocol for one to eight SNAP devices - multiple IPS units with
;  different protocol values may be set up for each SNAP device if desired
;	c{ES:EDI} = Address of data block
;	CALL	ipsaddunit
;	c{EAX} = Error code
;	c{EBX} = Status bits
;	c{ECX} = Item count

$$$=!0
FRM ipau_snapnum , 4t		;Number of SNAP devices specified
FRM ipau_unit    , 4t		;Unit number
FRM ipau_netnum  , 4t
FRM ipau_count   , 4t
FRM ipau_ndb     , 4t		;Offset of NDB
FRM ipau_sdb     , 4t*8t	;SDB offsets
FRM ipau_pdb     , 4t*8t	;Offset of PDBs
FRM ipau_icmptpdb, 4t*8t	;Offset of ICMP TPDBs
ipau_SIZE=!$$$

	DATA

	.MOD	4
ipsaublk:				;Parameter description block for
 DCHARBGN  3, knlDCharValCom##		;  adduparms
 DCHARENT  UNIT   , DECV, ,  1, 0, 0, ipsauunit    , 0
 DCHARENT  SNAPDEV, TEXT, , 16, 0, 0, ipsausnapdev , 0

	CODE

	IFFAULT	6$
ipsaddunit:
	LFSL	EDX, iorb_buffer2.B[ESI]
	CALL	knlGiveQel##		;Give up the IORB
	ENTER	ipau_SIZE, 0		;Allocate our stack frame
	CLRL	EAX
	LEAL	EDI, -ipau_SIZE[EBP]	;Clear our stack frame
	MOVL	ECX, #ipau_SIZE/4
	PUSHL	SS
	POPL	ES
	CLD
	RSTOSL	[EDI]
	DECL	ipau_unit.B[EBP]	;Indicate illegal unit number
	MOVL	EBX, #ipsaublk
	MOVB	AL, #0			;Fail on bad names
	CALL	knlAddUnitChar##	;Process the characteristics
	JC	4$.S			;If error
	CMPL	ipau_snapnum.B[EBP], #0.B ;Was at least one SNAP device
					  ;  specified?
	JE	2$.S			;No
	CMPL	ipau_unit.B[EBP], #0.B	;Yes - was a unit number specified?
	JGE	20$.S			;Yes
2$:	MOVL	EAX, #ER_CHARM		;No - fail
4$:	MOVL	EBX, #QSTS$DONE
	CLRL	ECX
	LEAVE
	TOFORK
	RET
		
;Here if address error with the IORB

	FAULTHDR
6$:	CALL	knlGiveQel##
	MOVL	EAX, #ER_ADRER
	JMP	4$.S

;Here if cannot allocate exec memory

10$:	MOVL	EBX, knlSvDataPnt##	;Give up all the memory we got here!
	CALL	knlGiveXMem##
14$:	CALL	knlGiveXRes##
	JMP	4$.S

;Here if device unit is already defined

16$:	MOVL	EAX, #ER_DUADF
	JMP	4$.S

;Here if have all required characteristics - we now allocate the following:
;	NDB for the network
;	PDB (IPS) for each SNAP device
;	TPDB (ICMP) for each SNAP device

20$:	MOVB	AL, ipau_unit.B[EBP]	;See if this unit is already defined
	MOVB	CL, #0
	MOVL	EDI, xosipsPdbHead
	CALL	xosnetSrchBlk2##
	JE	16$.S
	CALL	knlGetXRes##		;Get exec memory resource
	MOVL	ECX, #ndb_SIZE+ndb_mSIZE ;Allocate a network data block (NDB)
	CALL	knlGetXMem##
	JC	14$.S			;If error
	LEAL	EBX, ndb_mSIZE.B[EDI]
	MOVL	ipau_ndb.B[EBP], EBX
	MOVL	ndb_magic.B[EBX], #'NDB*'
	MOVL	ndb_rtmax.B[EBX], #20t	;Store default routing table maximum
	MOVL	EAX, ipau_snapnum.B[EBP] ;Store number of SNAP devices
	MOVL	ndb_numpdb.B[EBX], EAX
	MOVL	ipau_count.B[EBP], EAX
	MOVL	ipau_netnum.B[EBP], #1
	INCL	numndb			;Count the NDB

;Now allocate one PDB (IPS) and TPDB (ICMP) for each SNAP device

22$:	MOVL	ECX, #pdb_ipSIZE+pdb_mSIZE ;Allocate a PDB (Protocol Data Block)
	CALL	knlGetXMem##
	JC	10$.S			;If error
	LEAL	EBX, pdb_mSIZE[EDI]
	MOVL	pdb_magic.B[EBX], #'PDB*'
	MOVL	EAX, ipau_netnum.B[EBP]
	MOVL	ipau_pdb-4.B[EBP+EAX*4], EBX
	MOVL	ESI, ipau_sdb-4.B[EBP+EAX*4]
	MOVL	pdb_sdb.B[EBX], ESI	;Store offset of SDB in the PDB
	MOVL	EAX, sdb_maxpktsz.B[ESI] ;Store maximum packet size
	SUBL	EAX, #ip_options.B
	MOVL	pdb_ipmaxpktsz[EBX], EAX
	MOVL	EAX, sdb_snaphdrsz.B[ESI] ;Store header size
	ADDL	EAX, #ip_options.B
	MOVL	pdb_iphdrsz[EBX],EAX
	MOVL	EAX, sdb_idb.B[ESI]	;Store offset of IDB in the PDB
	MOVL	pdb_idb.B[EBX], EAX
	MOVL	pdb_pdb.B[EBX], EBX
	MOVL	pdb_ccb.B[EBX], #ipsccb
	MOVL	pdb_dcbsizex.B[EBX], #dcb_ipsINDEX
	MOVL	pdb_disp.B[EBX], #ipsdisp
	MOVL	EAX, ipau_unit.B[EBP]	;Get unit number
	MOVB	pdb_unit.B[EBX], AL
	MOVL	pdb_name.B[EBX], #'IPS'	;Put name into the PDB
	PUSHL	EBX
	ADDL	EBX, #pdb_name+3.B	;Add unit number
	CALL	knlPutDecNmbr##
	MOVL	EDX, ipau_netnum.B[EBP]	;Add network letter
	LEAL	EAX, 'A'-1.B[EDX]	;Change to network letter
	MOVB	[EBX], AL
	POPL	EBX
	MOVB	pdb_netnum.B[EBX], DL
	MOVB	pdb_plen.B[EBX], #4	;Store length of our protocol address
	MOVL	ECX, ipau_ndb.B[EBP]	;Store offset of NDB in the PDB
	MOVL	pdb_ipndb[EBX], ECX
	MOVL	ndb_pdb-4.B[ECX+EDX*4], EBX ;Store offset of PDB in the NDB	STC
	RCLL	ndb_subumask.B[ECX], #1	;Add a bit to the sub-unit mask
	MOVL	ECX, #tpdb_icmpSIZE+tpdb_mSIZE ;Allocate space for an ICMP TPDB
	CALL	knlGetXMem##
	JC	10$			;If error
	ADDL	EDI, #tpdb_mSIZE.B
	MOVL	ipau_icmptpdb-4.B[EBP+EDX*4], EDI
	MOVL	tpdb_magic.B[EDI], #'TPDB'
	MOVL	tpdb_rcvdisp.B[EDI], #icmprcv#
	MOVL	EAX, ipau_icmptpdb.B[EBP] ;Store offset of first TPDB
	MOVL	tpdb_ftpdb.B[EDI], EAX
	MOVL	EAX, ipau_pdb-4.B[EBP+EDX*4]
	MOVL	tpdb_pdb.B[EDI], EAX	;Store offset of the PDB
	MOVL	pdb_iptpdbhead.B[EAX], EDI
	MOVL	tpdb_ipprot.B[EDI], #IPP_ICMP ;Store protocol number
	MOVL	tpdb_ccb.B[EDI], #icmpccb
	MOVL	tpdb_dcbsizex.B[EDI], #dcb_icmpINDEX
	MOVL	tpdb_name+0.B[EDI], #'IC'
	MOVL	EAX, pdb_name+2.B[EAX]
	MOVL	tpdb_name+2.B[EDI], EAX
	MOVB	AL, ipau_unit.B[EBP]
	MOVB	tpdb_unit.B[EDI], AL
	MOVB	AL, ipau_netnum.B[EBP]	;Store network number
	MOVB	tpdb_netnum.B[EDI], AL
	INCL	ipau_netnum.B[EBP]	;Bump network number
	DECL	ipau_count.B[EBP]
	JNE	22$			;Continue if more to do

;When get here, we have allocated all of the memory we need - finally, we link
;  the data blocks we have built into the lists for the system (note that we
;  do this backwards so that the lists will be in the right order!)

	CALL	knlGiveXRes##
	MOVL	ECX, ipau_snapnum.B[EBP]

28$:	MOVL	EBX, ipau_pdb-4.B[EBP+ECX*4] ;Get offset of PDB
	MOVL	EAX, xosipsPdbHead		;Link into the IPS PDB list
	MOVL	xosipsPdbHead, EBX
	MOVL	pdb_nextd.B[EBX], EAX
	MOVL	EBX, ipau_icmptpdb-4[EBP+ECX*4] ;Get offset of ICMP TPDB
	MOVL	EAX, xosipsIcmpTpdbHead# ;Link into the ICMP TPDB list
	MOVL	xosipsIcmpTpdbHead, EBX
	MOVL	tpdb_nextd.B[EBX], EAX
	DECL	ECX			;More to do?
	JNE	28$			;Yes - continue
	MOVL	EBX, ipau_ndb.B[EBP]	;No - link the NDB into our list of
	MOVL	EAX, lastndb		;  NDBs
	TESTL	EAX, EAX
	JE	32$.S
	MOVL	[EAX], EBX
	JMP	34$.S

32$:	MOVL	firstndb, EBX
34$:	MOVL	lastndb, EBX
	CLRL	EAX			;Finished
	MOVL	EBX, #QSTS$DONE
	MOVL	ECX, #1
	LEAVE
	TOFORK
	RET
.PAGE
	.SBTTL	xosipsAuIpsDev - Subroutine to process IPSDEV addunit characteristic

;Subroutine to process IPSDEV addunit characteristic for devices which use IP
;  as their underlying protocol
;	c{EAX} = Current IPS PDB offset
;	CALL	xosipsAuIpsDev
;	C:set = Error
;	  c{EAX} = Error code
;	C:clr = Normal
;	  c{EDI} = Offset of IPS PDB

xosipsAuIpsDev:
	TESTL	EAX, EAX		;Alreay have an IPS PDB?
	JNE	2$.S			;Yes - fail
	MOVL	EBX, #knlDcValue##	;No
	PUSHL	12t.B[EBX]
	PUSHL	8.B[EBX]
	PUSHL	4.B[EBX]
	MOVL	EAX, [EBX]
	PUSHL	EAX
	ANDL	EAX, #0FFFFFFh
	CMPL	EAX, #'IPS'
	JNE	4$.S
	LEAL	ECX, 3.B[ESP]
	MOVL	EDI, xosipsPdbHead	;Get offset of first PDB
	CALL	xosnetSrchBlock##	;Find the correct PDB
	JNE	4$.S			;If can't find it
	CMPB	SS:[ECX], #0		;Did he specify a specific network?
	JNE	4$.S			;Yes - fail
	ADDL	ESP, #16t.B
	RET

;Here if more than one IPS device specified

2$:	MOVL	EAX, #ER_TMDDV
	STC
	RET

;Here if cannot find requested IPS device

4$:	ADDL	ESP, #16t.B
	JMP	knlBadCharV##
.PAGE
	.SBTTL	xosipsNewTransProt - Subroutine to create new IPS transport level protocol

;Subroutine to create new IPS transport level protocol
;	c{{ESP}+36t} = Unit number
;	c{{ESP}+32t} = Size for TPDB
;	c{{ESP}+28t} = Base name (2 to 4 characters)
;	c{{ESP}+24t} = Size index for DCB
;	c{{ESP}+20t} = Size for hash table (0 if none)
;	c{{ESP}+16t} = Offset in TPDB of hash table pointer
;	c{{ESP}+12t} = Offset of receive routine
;	c{{ESP}+8t}  = Offset of CCB
;	c{{ESP}+4t}  = Offset of PDB for first interface
;	c{{ESP}+0t}  = Offset of TPDB list head pointer for transport protocol
;	CALL	xosipsNewTransProt
;	C:set = Error
;	  c{EAX} = Error code
;	C:clr = Normal
;	  c{EDI} = Offset of first new TPDB, still have exec memory resource

mt_unit   =!44t		;Unit number
mt_tpdbsz =!40t		;Size of a TPDB
mt_name   =!36t		;Base name
mt_sizex  =!32t		;Size index for DCB
mt_hashsz =!28t		;Size for hash table
mt_hashpnt=!24t		;Offset in TPDB for hash table pointer
mt_rcvdisp=!20t
mt_ccb    =!16t		;Offset of CCB
mt_pdb    =!12t		;Offset of PDB for first interface
mt_tpdbhp =!8t		;Offset of TPDB list head pointer

$$$=!0
FRM mt_index  , 4t
FRM mt_numpdb , 4t	;Number of PDBs
FRM mt_hashtbl, 4t	;Offset of hash table
FRM mt_tpdb   , 4t*8t
mt_SIZE=!$$$

xosipsNewTransProt:
	ENTER	mt_SIZE, 0
	MOVL	ESI, mt_tpdbhp.B[EBP]
	MOVL	EDI, [ESI]		;Look for TPDB for transport protocol
	MOVB	AL, mt_unit.B[EBP]
2$:	TESTL	EDI, EDI
	JE	6$.S
	CMPB	tpdb_unit.B[EDI], AL
	JE	4$.S
	MOVL	EDI, tpdb_nextd.B[EDI]
	JMP	2$.S

;Here if device is already defined

4$:	MOVL	EAX, #ER_DUADF
	LEAVE
	STC
	RET	40t

;Here if desired tranport protocol is not now defined - first allocate space
;  for the TPDBs for the protocol

6$:	CALL	knlGetXRes##		;Get the exec memory resource
	MOVL	EDI, mt_pdb.B[EBP]
	MOVL	ESI, pdb_ipndb[EDI]
	MOVL	EAX, ndb_numpdb.B[ESI]	;Get number of PDBs
	MOVL	mt_numpdb.B[EBP], EAX
	MOVL	mt_index.B[EBP], #1
	CLRL	EDI
	MOVL	ECX, mt_hashsz.B[EBP]
	JREGZ	ECX, 8$
	CALL	knlGetXMem##
	JC	12$.S
8$:	MOVL	mt_hashtbl.B[EBP], EDI
10$:	MOVL	ECX, mt_tpdbsz.B[EBP]	;Get size for a TPDB
	ADDL	ECX, #tpdb_mSIZE.B
	CALL	knlGetXMem##		;Allocate memory
12$:	JC	22$			;If error
	ADDL	EDI, #tpdb_mSIZE.B
	MOVL	EAX, mt_rcvdisp.B[EBP]	;Store receive dispatch offset
	MOVL	tpdb_rcvdisp.B[EDI], EAX
	MOVL	tpdb_magic.B[EDI], #'TPDB'
	MOVL	EDX, mt_name.B[EBP]	;Get name
	MOVL	EAX, mt_unit.B[EBP]	;Get unit number
	MOVB	tpdb_unit.B[EDI], AL
	MOVL	tpdb_name.B[EDI], EDX
	LEAL	EBX, tpdb_name+2.B[EDI]
	TESTL	EDX, #0FF0000h		;Have a 3 or 4 character name?
	JE	16$.S			;No
	INCL	EBX			;Yes
	TESTL	EDX, #0FF000000h	;Have a 4 character name?
	JE	16$.S			;No
	INCL	EBX			;Yes
16$:	CALL	knlPutDecNmbr##		;Insert number into name
	MOVL	EAX, mt_index.B[EBP]
	MOVB	tpdb_netnum.B[EDI], AL
	ADDB	AL, #'A'-1		;Insert network letter into name
	MOVB	[EBX], AL
	MOVL	ECX, mt_index.B[EBP]
	MOVL	mt_tpdb-4.B[EBP+ECX*4], EDI
	MOVL	EAX, mt_sizex.B[EBP]	;Store size index for DCBs
	MOVL	tpdb_dcbsizex.B[EDI], EAX
	MOVL	EAX, mt_ccb.B[EBP]	;Store offset of the CCB
	MOVL	tpdb_ccb.B[EDI], EAX
	MOVL	EAX, mt_tpdb+0.B[EBP]	;Store offset of first TPDB
	MOVL	tpdb_ftpdb.B[EDI], EAX
	MOVL	EAX, mt_hashtbl.B[EBP]	;Store offset of hash table
	MOVL	EDX, mt_hashpnt.B[EBP]
	MOVL	[EDI+EDX], EAX
	MOVL	EAX, ndb_pdb-4.B[ESI+ECX*4] ;Get offset of corresponding PDB
	MOVL	tpdb_pdb.B[EDI], EAX	;Store it in the PDB
	DECL	[EDI]			;Indicate not in an IP protocol list
	INCL	mt_index.B[EBP]
	CMPL	ECX, mt_numpdb.B[EBP]
	JB	10$
	CALL	knlGiveXRes##		;Give up the exec memory resource

;Here with all TPDBs allocated - now link them into the device list for the
;  protocol

	MOVL	ESI, mt_tpdbhp.B[EBP]
18$:	MOVL	EDI, mt_tpdb-4.B[EBP+ECX*4]
	MOVL	EAX, [ESI]
	MOVL	[ESI], EDI
	MOVL	tpdb_nextd.B[EDI], EAX
	LOOP	ECX, 18$
	MOVL	EDI, mt_tpdb+0.B[EBP]	;Return offset of first TPDB
	CLC
20$:	LEAVE
	RET	40t

;Here if error allocating memory to TPDBs

22$:	MOVL	EBX, knlSvDataPnt##	;Give up all the memory we got here!
	CALL	knlGiveXMem##
	CALL	knlGiveXRes##		;Give up the exec memory resource
	JMP	20$.S
.PAGE
	.SBTTL	xosipsMakeDev - Subroutine to create new IPS application level device

;Subroutine to create new IPS application level device
;	c{{ESP}+24t} = Unit number
;	c{{ESP}+20t} = Size for DPDB
;	c{{ESP}+16t} = Base name (3 or 4 characters)
;	c{{ESP}+12t} = Size index for DCB
;	c{{ESP}+8t}  = Offset of CCB
;	c{{ESP}+4t}  = Offset of TPDB for first interface
;	c{{ESP}+0t}  = Offset of DPDB list head pointer for device
;	CALL	xosipsMakeDev
;	C:set = Error
;	  c{EAX} = Error code
;	C:clr = Normal
;	  c{EDI} = Offset of first new DPDB, still have exec memory resource

md_unit   =!32t		;Unit number
md_dpdbsz =!28t		;Size of a TPDB
md_name   =!24t		;Base name
md_sizex  =!20t		;Size index for DCB
md_ccb    =!16t		;Offset of CCB
md_ftpdb  =!12t		;Offset of TPDB for first interface
md_dpdbhp =!8t		;Offset of DPDB list head pointer

$$$=!0
FRM md_index , 4t
FRM md_number, 4t	;Number of PDBs for device
FRM md_ctpdb , 4t	;Offset of current TPDB
FRM md_dpdb  , 4t*8t
md_SIZE=!$$$

xosipsMakeDev:
	ENTER	md_SIZE, 0
	MOVL	EDI, md_ftpdb.B[EBP]
	CALL	finddunit		;Find TPDB for transport protocol device
	JNE	8$.S			;Fail if not found
	MOVL	md_ftpdb.B[EBP], EDI	;Save offset of transport device TPDB
	MOVL	md_ctpdb.B[EBP], EDI
	MOVL	EAX, tpdb_pdb.B[EDI]	;Get number of PDBs
	MOVL	EAX, pdb_ipndb[EAX]
	MOVL	EAX, ndb_numpdb.B[EAX]
	MOVL	md_number.B[EBP], EAX

;Here with DPDB for transport protocol device found - now search the list of
;  specific DPDBs for the device we want to create and see if the device
;  already exists

	MOVZBL	EAX, md_unit.B[EBP]	;Restore unit number
	MOVL	EDI, md_dpdbhp.B[EBP]	;Get offset of first DPDB
	MOVL	EDI, [EDI]
	CALL	finddunit		;Try to find the device
	JNE	12$.S			;OK if not there
	MOVL	EAX, #ER_DUADF		;Found it - already defined!
	JMP	10$.S

;Here if protocol device does not exist

8$:	MOVL	EAX, #ER_NSDEV
10$:	LEAVE
	STC
	RET	28t

;Here if device to be created does not now exist - its OK to create the new
;  device - to do this we must create one DPDB for each SNAP device used by
;  the device's network - first we allocate the memory, then we set up and
;  link the DPDBs

12$:	CALL	knlGetXRes##		;Get exec memory resource
	CLRL	EAX
	MOVL	md_index.B[EBP], EAX
14$:	INCL	md_index.B[EBP]
	MOVZBL	ECX, md_dpdbsz.B[EBP]	;Get size for the DPDB
	SHLL	ECX, #2
	ADDL	ECX, #dpdb_mSIZE.B
	CALL	knlGetXMem##		;Allocate memory
	JC	22$			;If error
	ADDL	EDI, #dpdb_mSIZE.B
	MOVL	dpdb_magic.B[EDI], #'DPDB'
	MOVL	EAX, md_unit.B[EBP]
	MOVB	dpdb_unit.B[EDI], AL
	MOVL	EDX, md_name.B[EBP]
	MOVL	dpdb_name.B[EDI], EDX
	LEAL	EBX, dpdb_name+3.B[EDI]
	TESTL	EDX, #0FF000000h	;Have a 4 character name?
	JE	16$.S			;No
	INCL	EBX			;Yes
16$:	CALL	knlPutDecNmbr##		;Insert unit number into name
	MOVL	ECX, md_index.B[EBP]
	LEAL	EAX, 'A'-1.B[ECX]	;Insert network letter into name
	MOVB	[EBX], AL
	MOVL	md_dpdb-4.B[EBP+ECX*4], EDI
	MOVB	dpdb_netnum.B[EDI], CL	;Store network number
	MOVL	EAX, md_sizex.B[EBP]	;Store size index for DCBs
	MOVL	dpdb_dcbsizex.B[EDI], EAX
	MOVL	EAX, md_ccb.B[EBP]	;Store offset of the CCB
	MOVL	dpdb_ccb.B[EDI], EAX
	MOVL	EAX, md_dpdb+0.B[EBP]	;Store offset of first DPDB
	MOVL	dpdb_fdpdb.B[EDI], EAX
	MOVL	EAX, md_ctpdb.B[EBP]	;Store offset of corresponding TPDB
	MOVL	dpdb_tpdb.B[EDI], EAX
	MOVL	EDX, tpdb_nextd.B[EAX]	;Advance to next TPDB
	MOVL	md_ctpdb.B[EBP], EDX
	MOVL	EAX, tpdb_pdb.B[EAX]	;Store offset of corresponding PDB
	MOVL	dpdb_pdb.B[EDI], EAX
	DECL	[EDI]			;Indicate not in a protocol list
	CMPL	ECX, md_number.B[EBP]
	JB	14$.S

;Here with all memory allocated for the DPDBs - now we must link them into the
;  device list

18$:	MOVL	EAX, md_index.B[EBP]
	MOVL	EDI, md_dpdb-4.B[EBP+EAX*4]
	MOVL	EBX, md_dpdbhp.B[EBP]	;Link into DPDB list
	MOVL	EAX, [EBX]
	MOVL	[EBX], EDI
	MOVL	dpdb_nextd.B[EDI], EAX
	DECL	md_index.B[EBP]		;Continue if more to do
	JNE	18$.S
	MOVL	EDI, md_dpdb+0.B[EBP]	;Return offset of first DPDB
	CLC
20$:	LEAVE
	RET	28t			;Finished

;Here if error when allocating memory

22$:	MOVL	EBX, knlSvDataPnt##	;Give up all the memory we got here!
	CALL	knlGiveXMem##
	CALL	knlGiveXRes##		;Give up the exec memory resource
	STC				;Indicate error
	JMP	20$.S
.PAGE
;Subroutine to find network device in a TPDB or DPDB list
;	c{EDI} = Offset of first TPDB or DPDB
;	CALL	finddunit
;	Z:set = Device found
;	  c{EDI} = Offset of TPDB or DPDB
;	Z:clr = Not found

finddunit:
	MOVB	AL, md_unit.B[EBP]
4$:	TESTL	EDI, EDI
	JE	6$.S
	CMPB	dpdb_unit.B[EDI], AL
	JE	8$.S
	MOVL	EDI, dpdb_nextd.B[EDI]
	JMP	4$.S

6$:	TESTL	ESP, ESP
8$:	RET
.PAGE
;Subroutine called by adduparms for the "UNIT" parameter

ipsauunit:
	CMPL	EAX, #9t.B		;Valid value?
	JA	badcharv		;No - fail
	MOVL	ipau_unit.B[EBP], EAX	;Yes - store it
	CLC
	RET

;Subroutine called by adduparms for the "SNAPDEV" parameter

ipsausnapdev:
	MOVL	ESI, xossnpSdbHead##	;Get offset of first SDB
2$:	TESTL	ESI, ESI		;Have another to check?
	JE	8$.S			;If no more
	CMPL	sdb_name+0.B[ESI], EAX	;This one?
	JNE	4$.S			;No
	CMPL	sdb_name+4.B[ESI], EDX	;Maybe
	JE	6$.S			;Yes
4$:	MOVL	ESI, sdb_nextd.B[ESI]	;No - advance to next
	JMP	2$.S			;Continue

;Here with SDB for the requested SNAP device

6$:	MOVL	EAX, ipau_snapnum.B[EBP] ;Get current SNAP number
	CMPL	EAX, #7.B		;Already have maximum number?
	JAE	10$.S			;Yes - fail
	INCL	ipau_snapnum.B[EBP]	;No - bump number
	MOVL	ipau_sdb.B[EBP+EAX*4], ESI ;Store SDB offset
	CLC
	RET

;Here if cannot find requested SNAP device

8$:	MOVL	EAX, #ER_NSDEV
	STC
	RET

;Here if have too many SNAP devices specified

10$:	MOVL	EAX, #ER_TMDDV
	STC
	RET
.PAGE
	.SBTTL	xosipsSetIpProt - Subroutine to change IP protocol value

;Subroutine to change IP protocol value for a transport level device (this
;  is normally called when the IPPORT characteristic is changed)
;	c(AL)  = New protocol value
;	c{EDI} = Offset of DCB
;	CALL	xosipsSetIpProt
;	C:set = Error
;	  c{EAX} = Error code
;	C:clr = Normal

xosipsSetIpProt:
	MOVL	EBX, dcb_ipstpdb.B[EDI]	;Get offset of the TPDB
	MOVL	EBX, tpdb_ftpdb.B[EBX]	;Make sure have first TPDB for device
	MOVB	AH, tpdb_unit.B[EBX]	;Get unit number
	CMPL	[EBX], #-1.B 		;Really in use?
	JE	14$.S			;No
4$:	MOVB	tpdb_ipprot.B[EBX], #0	;Yes - clear the IP protocol value
	MOVL	ESI, tpdb_pdb.B[EBX]	;Get offset of the PDB
	LEAL	EDX, pdb_iptpdbhead.B[ESI] ;Get offset of head pointer
6$:	CMPL	[EDX], EBX		;This one?
	JE	8$.S			;Yes
	MOVL	EDX, [EDX]		;No - advance to next
	TESTL	EDX, EDX
	JNE	6$.S
	CRASH	BIPL			;[Bad IP protocol List]

8$:	MOVL	ECX, [EBX]		;Remove this PDB from the list
	MOVL	[EDX], ECX
10$:	MOVL	[EBX], #-1		;Indicate not in use
	MOVL	EBX, tpdb_nextd.B[EBX]	;Advance to next TPDB
	TESTL	EBX, EBX		;Have another?
	JE	12$.S			;No
	CMPB	tpdb_unit.B[EBX], AH	;Yes - for same unit?
	JE	4$.S			;Yes - continue
12$:	MOVL	EBX, dcb_ipstpdb.B[EDI]	;Restore first TPDB for unit
	MOVL	EBX, tpdb_ftpdb.B[EBX]
14$:	CMPB	AL, #0			;Clearing the protocol?
	JE	24$.S			;Yes - finished now
16$:	MOVB	tpdb_ipprot.B[EBX], AL	;Store new IP protocol value
	MOVL	ESI, tpdb_pdb.B[EBX]	;Get offset of PDB
	LEAL	EDX, pdb_iptpdbhead.B[ESI] ;Get offset of head pointer
18$:	MOVL	ECX, [EDX]		;Get next TPDB
	JREGZ	ECX, 22$		;If no more
	CMPB	AL, tpdb_ipprot.B[ECX]	;Does it go here?
	JBE	20$.S			;Yes
	MOVL	EDX, ECX 		;No - advance to next TPDB
	JMP	18$.S			;Continue

;Here with position for new entry

20$:	JE	knlBadCharV##		;If duplicate
22$:	MOVL	[EBX], ECX		;Insert into list
	MOVL	[EDX], EBX
	MOVL	EBX, tpdb_nextd.B[EBX]	;Advance to next TPDB
	TESTL	EBX, EBX		;Have another?
	JE	24$.S			;No - finished
	CMPB	tpdb_unit.B[EBX], AH	;Yes - for same unit?
	JE	16$.S			;Yes - continue
24$:	CLRL	EAX			;No - finished
	RET
.PAGE
	.SBTTL	ipsunitinfo - IPS phyical unit information function

;Here for the get physical unit information class function (CL_PUNITS) for the
;  IPS class
;	c{EBX} = Offset of CCB
;	c{ESI} = Offset of IORB
;	CALL	ipsunitinfo
;	c{EAX} = Error code
;	c{EBX} = Status bits
;	c{ECX} = Item count
;  A zero count (iorb_count) requests that the number of units only be returned
;    in qab_amount.  A non-zero count indicates that a buffer is available for
;    that many unit names (8 characters each, null padded).

ipsunitinfo:
	MOVL	ECX, numndb		;Get number of units
	CMPL	iorb_count.B[ESI], #0.B	;Want number of units only?
	JE	8$.S			;Yes
	CMPL	iorb_count.B[ESI], ECX	;Is the buffer big enough?
	JA	14$.S			;No - fail
	IFFAULT	knlQAdrErr0##
	LESL	EDI, iorb_buffer2.B[ESI] ;Yes - get buffer address
	MOVL	EBX, firstndb		;Yes - point to first NDB
2$:	MOVL	EAX, ndb_pdb.B[EBX]	;Point to first PDB
	MOVL	EAX, pdb_name+0.B[EAX]	;Store name
	IFFAULT	knlQAdrErr0##
	STOSL	[EDI]
	CLRL	EAX
	IFFAULT	knlQAdrErr0##
	STOSL	[EDI]
	MOVL	EBX, [EBX]		;Get next NDB
	TESTL	EBX, EBX
	JNE	2$.S			;Continue if another
	MOVL	ECX, numndb		;Get number of names we gave him
8$:	CLRL	EAX
10$:	CALL	knlGiveQel##
	MOVL	EBX, #QSTS$DONE		;Yes
	RET

;Here if count is too small

14$:	MOVL	EAX, #ER_ICDEV
	CLRL	ECX
	JMP	10$.S
.PAGE
