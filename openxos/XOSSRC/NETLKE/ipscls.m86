.PAGE
	.SBTTL	rtuse - Here for the IOPAR_IPS_RTUSE IO parameter

;Here to get the value of the IOPAR_IPS_RTUSE IO parameter - this returns the
;  current number of routing table entries (this is the same as the RTUSE
;  device characteristic)

	DPARMHDR  GET, DECV
rtuse:	MOVL	EDX, dcb_ipsndb.B[EDI]	;Get offset of NDB
	MOVL	EAX, ndb_inuse.B[EDX]	;Get number of routing table entries
	JMP	knlStrParm4##
.PAGE
	.SBTTL	rtdata - Here for the IOPAR_IPS_RTDATA IO parameter

;Here to get the value of the IOPAR_IPS_RTDATA IO parameter - this returns the
;  contents of the routing table as structured binary data

$$$=!0
FRM grtd_avail, 4t
FRM grtd_num  , 4t
FRM grtd_size , 8t		;Offset of word to receive size
grtd_SIZE=!$$$

	DPARMHDR  GET, DATAS
rtdata:	ENTER	grtd_SIZE, 0
	MOVL	grtd_avail.B[EBP], ECX	;Store space available
	MOVL	grtd_size+0.B[EBP], EDX	;Store offset of word to receive size
	MOVL	grtd_size+4.B[EBP], FS
	CLRL	EAX
	MOVL	grtd_num.B[EBP], EAX	;Clear amount stored
	MOVL	EDX, dcb_ipsndb.B[EDI]	;Get offset of NDB
	MOVL	EDI, EBX		;Get buffer offset in right register
	MOVL	ECX, ndb_rthead.B[EDX]	;Get offset of first routing table entry
	TESTL	ECX, ECX
	JE	20$			;If none at all
4$:	MOVZBL	EAX, rt_type.B[ECX]	;Get routing table entry type
	CMPB	AL, #RTT_PING		;Is it really a routing table entry?
	JAE	16$.S			;No
	SUBL	grtd_avail.B[EBP], #20t.B ;Yes - do we have room for it?
	JLE	22$			;No - fail!
	MOVB	AH, rt_pdbx.B[ECX]	;Yes - get SNAP device index
	STOSL	[EDI]			;Store type
	MOVL	EAX, rt_ipaddr.B[ECX]	;Store IP address
	STOSL	[EDI]
	MOVL	EAX, rt_t2lcnt.B[ECX]	;Store time-to-live value
	STOSL	[EDI]
	MOVB	AL, rt_type.B[ECX]	;Get entry type
	CMPB	AL, #RTT_LOCAL
	JE	6$.S
	CMPB	AL, #RTT_ROUTER
	JNE	8$.S
6$:	MOVL	EAX, rt_hwaddr+0.B[ECX]
	STOSL	[EDI]
	MOVL	EAX, rt_hwaddr+4.B[ECX]
	JMP	14$.S

8$:	CMPB	AL, #RTT_FOREIGN
	JE	10$.S
	CMPB	AL, #RTT_FPENDING
	JE	10$.S
	CMPB	AL, #RTT_DPENDING
	JNE	12$.S
10$:	MOVL	EAX, rt_router.B[ECX]	;Is there an assoicated router?
	TESTL	EAX, EAX
	JE	12$.S			;No
	MOVL	EAX, rt_ipaddr.B[EAX]	;Yes - get its IP address
	STOSL	[EDI]
	CLRL	EAX
	JMP	14$.S

12$:	CLRL	EAX
	STOSL	[EDI]
14$:	STOSL	[EDI]
	ADDL	grtd_num.B[EBP], #20t.B	;Count it
16$:	MOVL	ECX, rt_next.B[ECX]	;Advance to next entry
	TESTL	ECX, ECX
	JNE	4$			;Continue if more to do
	MOVL	ECX, grtd_num.B[EBP]	;Finished - get size
20$:	LESL	EDX, grtd_size.B[EBP]	;Get address for size
	MOVW	ES:[EDX], CX		;Store size
	LEAVE
	RET

22$:	MOVL	EAX, #ER_DATTR
	LEAVE
	STC
	RET
.PAGE
	.SBTTL	xosipsParseAddress - Subroutine to parse IP address

;Subroutine to parse IP address string. The subroutine handles IP addresses
;  and domain names with an optional port specification. Must be called in
;  extended fork context.
;	c{ES:EBX} = Address of address string
;	c{EDI}    = Offset of DCB
;	CALL	xosipsParseAddress
;	C:set = Error
;	  c{EAX} = Error code
;	C:clr = Normal, dcb_ipsrmtaddr and dcb_ipsrmtport have been updated
;		  if necessary

;The address string has the following formats: ({} indicates optional items)
;	nn{.nn{.nn{.nn}}}{|port}
;	[nn{.nn{.nn{.nn}}}]{|port}
;	dddd{.dddd{.dddd{...}}}{|port}
;  Where nn is a decimal between 0 and 255, dddd is a domain name element, and
;    port is a port number between 1 and 65535.
;  Note that the first format is obsolete and should not be used in new
;    software!

xosipsParseAddress:
	CLRL	EDX
	CLRL	ECX
	MOVB	AL, ES:[EBX]
	CMPB	AL, #0
	JE	20$.S
	CMPB	AL, #'['
	JE	10$.S
	CMPB	AL, #'0'
	JB	12$.S
	CMPB	AL, #'9'
	JBE	12$.S

;Here if have a domain name!

	MOVL	EAX, #ER_NNSNA
	STC
	RET

;Here if have a delimited IP address

10$:	INCL	EBX
	MOVB	CH, #']'
	JMP	14$.S

;Here if have a non-delimited IP address

12$:	MOVB	CH, #'|'
14$:	MOVB	CL, #4			;IP address may contain up to 4
16$:	CALL	getval			;  fields
	RORL	EDX, #8
	CMPB	AL, CH			;End of IP address?
	JE	22$.S			;Yes
	CMPB	AL, #'.'		;No - is it the separator?
	JNE	18$.S			;No - fail
	DECB	CL
	JNE	16$.S			;Yes - continue
18$:	MOVL	EAX, #ER_NILAD
	STC
20$:	RET

;Here at end of IP address

22$:	CMPB	AL, #']'		;Delimited IP address?
	JNE	24$.S			;No
	MOVB	AL, ES:[EBX]		;Yes - get next character
	INCL	EBX
24$:	CLRL	ECX
	CMPB	AL, #'|'		;Port number next?
	JNE	28$.S			;No
26$:	MOVB	AL, ES:[EBX]		;Get character
	INCL	EBX			;Bump pointer
	CMPB	AL, #'0'		;Is it a digit?
	JB	28$.S			;No
	CMPB	AL, #'9'
	JA	28$.S
	ANDL	EAX, #0Fh.B		;Yes - get value
	IMULL	ECX, #10t.B		;And add into value
	ADDL	ECX, EAX
	JMP	26$.S

;Here at end of port number

28$:	CMPB	AL, #0			;End of string?
	JNE	18$.S			;No - error
	TESTL	ECX, #0FFFF0000h	;Yes - valid value?
	JNE	18$.S			;No
	MOVL	dcb_ipsrmtaddr[EDI], EDX ;Yes - store IP address
	ORL	ECX, ECX		;Was a port specified?
	JE	30$.S			;No
	XCHGB	CL, CH			;Yes - change to network byte order
	MOVW	dcb_ipsrmtport.B[EDI], CX ;Store value
30$:	RET				;Finished

;Subroutine to get value of next IP address field
;	c{EBX} = Offset of address string
;	CALL	getval
;	c(AL)  = Stopper character
;	c(BL)  = Value
;	c{EBX} = Offset of next character in address string

getval:	MOVB	AL, #0
32$:	MOVB	DL, ES:[EBX]		;Get character
	INCL	EBX			;Bump pointer
	CMPB	DL, #'0'		;Is it a digit?
	JB	34$.S			;No
	CMPB	DL, #'9'
	JA	34$.S
	ANDB	DL, #0Fh		;Yes - get value
	MULB	lit10			;And add into value for field
	ADDB	AL, DL
	JMP	32$.S

;Here if next character is not a digit

34$:	XCHGB	AL, DL			;Get things in right registers
	RET				;Return
.PAGE
	.SBTTL	xosipsStoreAddress - Subroutine to store IP address

;Subroutine to store IP address string. The string stored can always be used
;  as input to xosipsParseAddress. It has the format:
;	[nn.nn.nn.nn]|port
;  Where nn is a decimal between 0 and 255, and port is a port number between
;    1 and 65535.
;	c{ES:EBX} = Address of buffer
;	c{ECX}    = Length of buffer
;	c{EAX}    = IP address
;	c{EDX}    = Port number
;	CALL	xosipsParseAddress
;	C:set = Error
;	  c{EAX} = Error code
;	C:clr = Normal
;	c{ECX} = Length of string stored

$$$=!0
FRM sa_left, 4t
FRM sa_size, 4t
FRM sa_addr, 4t
FRM sa_port, 4t
sa_SIZE=!$$$

xosipsStoreAddress:
	DECL	ECX
	JS	6$.S
	ENTER	sa_SIZE, 0
	MOVL	sa_left.B[EBP], ECX
	MOVL	sa_size.B[EBP], ECX
	MOVL	sa_addr.B[EBP], EAX
	MOVL	sa_port.B[EBP], EDX
	MOVB	AL, #'['
	CALL	storechr
	MOVZBL	EAX, sa_addr+0.B[EBP]
	CALL	storedec
	MOVB	AL, #'.'
	CALL	storechr
	MOVZBL	EAX, sa_addr+1.B[EBP]
	CALL	storedec
	MOVB	AL, #'.'
	CALL	storechr
	MOVZBL	EAX, sa_addr+2.B[EBP]
	CALL	storedec
	MOVB	AL, #'.'
	CALL	storechr
	MOVZBL	EAX, sa_addr+3.B[EBP]
	CALL	storedec
	MOVB	AL, #']'
	CALL	storechr
	MOVB	AL, #'|'
	CALL	storechr
	MOVL	EAX, sa_port.B[EBP]
	CALL	storechr
4$:	MOVL	ES:[EBX], #0
	MOVL	EAX, sa_size.B[EBP]
	SUBL	EAX, ECX
	LEAVE
6$:	RET


storedec:
	CLRL	EDX
	DIVL	lit10
	PUSHL	EDX
	ORL	EAX, EAX
	JE	8$.S
	CALL	storedec
8$:	POPL	EAX
	ANDB	AL, #0Fh
	ADDB	AL, #'0'
storechr:
	JREGZ	ECX, 4$
	DECL	ECX
	MOVB	ES:[EBX], AL
	RET	
.PAGE
	.SBTTL	ipsrcv - Subroutine to process received IP packet

2$:	POPL	ECX
	POPL	ESI
4$:	INCL	pdb_ipcntpsltmn.B[EDI]
	JMP	7$.S

6$:	POPL	ECX
7$:	INCL	pdb_ipcntbadhdr.B[EDI]
8$:	CALL	xosnetGiveBufr##
	RET

10$:	POPL	ECX
	INCL	pdb_ipcntchksum.B[EDI]
	JMP	8$.S

;Subroutine to process received IP packet
;	c{EBX} = Offset of packet buffer
;	c{EDI} = Offset of PDB
;	c{ESI} = Offset of SDB
;	CALL	ipsrcv

ipsrcv:	CMPW	npb_magic.B[EBX], #NPB_MAGIC ;Is this really a network buffer?
	JE	12$.S			;Yes
	CRASH	NANB			[Not A Network Buffer]

12$:	MOVZBL	ECX, npb_npofs.B[EBX]
	NEGL	ECX
	ADDL	ECX, npb_count.B[EBX]	;Get length of IP part of packet
	CMPL	ECX, #20t.B		;Is packet big enough for an IP header?
	JL	4$.S			;No - ignore it!
	MOVZBL	EAX, npb_npofs.B[EBX]	;Yes - get offset of start of IP header
	LEAL	EDX, [EBX+EAX]
	PUSHL	ECX
	MOVB	AL, ip_vershlen.B[EDX]	;Get IP version number
	MOVB	CL, AL
	ANDB	AL, #0F0h
	CMPB	AL, #40h		;Is it right?
	JNE	6$.S			;No - ignore the packet
	ANDL	ECX, #0Fh.B		;Get number of longs in the header
	IMULL	EAX, ECX, #4t.B		;Get number of bytes in the header
	CMPL	[ESP], EAX		;Is the packet this long?
	JS	2$.S			;No - ignore it!
	ADDB	AL, npb_npofs.B[EBX]	;Yes - store offset of start of
	MOVB	npb_tpofs.B[EBX], AL	;  transport part of packet
	TESTB	pdb_ipchksumh[EDI], #PCSH$CHK ;Should we check the checksum
					      ;  value?
	JE	20$.S			;No - go on
	PUSHL	ESI			;Yes
	LEAL	ESI, 16t.B[EDX]		;Yes
	CLD
	LODSL	[ESI]			;Calculate checksum for first 5 longs
	ADDL	EAX, -20t.B[ESI]
	ADCL	EAX, #0.B
	ADDL	EAX, -16t.B[ESI]
	ADCL	EAX, #0.B
	ADDL	EAX, -12t.B[ESI]
	ADCL	EAX, #0.B
	ADDL	EAX, -8.B[ESI]
	ADCL	EAX, #0.B
	SUBL	ECX, #5.B		;Any more to do?
	JLE	16$.S			;No
14$:	ADDL	EAX, [ESI]		;Yes
	ADCL	EAX, #0.B
	ADDL	ESI, #4.B
	LOOP	ECX, 14$
16$:	MOVL	ECX, EAX		;Merge halves
	SHRL	ECX, #16t
	ADDW	AX, CX
	ADCW	AX, #0.B
	POPL	ESI
	INCW	AX			;Is it right?
	JNE	10$			;No
20$:	POPL	ECX
	MOVZWL	EAX, ip_tlength.B[EDX]	;Yes - Get total IP length
	XCHGB	AL, AH
	CMPL	EAX, ECX		;Is this packet big enough for this?
	JA	22$.S			;No - ignore this packet
	MOVL	ECX, EAX		;Yes - use this count now
	MOVZBL	EAX, npb_npofs.B[EBX]	;Add back in the length before the IP
	ADDL	ECX, EAX		;  header
	MOVL	npb_count.B[EBX], ECX
	TESTW	ip_fragment.B[EDX], #0FF3Fh ;Is this a fragmented packet?
	JE	rcvrdy			;No - go process the packet
	JMP	havfrag.S		;Yes - go handle that

;Here if packet size is less than the data length

22$:	INCL	pdb_ipcntpsltdl.B[EDI]
	CALL	xosnetGiveBufr##
	RET
.PAGE
;Here if we have part of a fragmented packet.  We first check our current
;  fragment list to see if we are already receiving this packet.  If so, we
;  check to see if this is a duplicate and, if not, link it to the list for
;  the packet.  If we are not receiving this packet, we start receiving it.

;Format and management of the fragmented packet list:
;  A list of fragmented packets is keep with head pointer pdb_ipfraghead and
;  link pointer npb_next.  For each fragmented packet, a sub-list of fragments
;  is keep with head pointer npb_link in the first fragment and link pointer
;  npb_link in each additional fragment.  Note that all fragments are actually
;  complete packets.  The list is ordered by fragment offset.  A timer is
;  maintained using npb_fragtmr in the first fragment as the counter.  The
;  timer is started when the first fragment for a packet is received.  If all
;  fragments have not been received before it times out, the fragments that
;  have been received are discarded.  The fragment offset, total IP packet
;  length, IP source address, and the packet ID value are copied to fixed
;  locations in the packet header for each fragment to make searching the
;  fragment list quicker.  When all fragments for a packet have been received,
;  the original packet is reconstructed and processed.

;	c{ECX} = Total size of packet (including ALL headers) according to the
;		   IP total length
;	c{EDX} = Offset of start of IP header

havfrag:INCL	pdb_ipcntnumfrag.B[EDI]	;Count the fragment
	MOVZWL	EAX, ip_fragment.B[EDX]	;Get fragment flags and offset
	XCHGB	AL, AH			;Fix up the byte order
	SHLL	EAX, #3t		;Times 8 gives byte offset and moves
					;  the flag bits to the high 16 bits
	MOVL	npb_fragofs.B[EBX], EAX	;Store this
	ADDW	CX, AX			;Store data length through the end of
	MOVL	npb_count.B[EBX], ECX	;  this fragment
	MOVZWL	EAX, ip_ident.B[EDX]	;Store packet ID value in header
	MOVL	npb_fragid.B[EBX], EAX
	MOVL	ECX, ip_srcipaddr.B[EDX] ;Store source IP address
	MOVL	npb_fragipa.B[EBX], ECX
	PUSHL	ESI
	LEAL	ESI, pdb_ipfraghead[EDI] ;Now search the fragment list to see
4$:	MOVL	EDX, [ESI]		 ;  if we are now receiving this packet
	TESTL	EDX, EDX
	JE	8$.S
	CMPL	EAX, npb_fragid.B[EDX]
	JNE	6$.S
	CMPL	ECX, npb_fragipa.B[EDX]
	JE	10$.S			;Found it
6$:	LEAL	ESI, npb_next.B[EDX]	;Not this one - get next
	JMP	4$.S			;Continue if more to check

;Here if we are not now receiving this packet - add it to the fragment list

8$:	MOVL	npb_fragtmr.B[EBX], #TIMEOUT_FRAG
	CLRL	EAX
	MOVL	npb_fragnxt.B[EBX], EAX
	MOVL	npb_next.B[EBX], EAX
	MOVL	[ESI], EBX
	POPL	ESI			;That all for now (since this is the
	RET				;  first fragment, the packet can't be
					;  complete now!)

;Here if we are currently receiving this packet - First search through the
;  fragments we have and find the place for this one.  Note that duplicate
;  and/or overlapping fragments are possible since it is valid to retransmit
;  a packet without changing its ID and different copies of the same packet
;  may be fragmented differently.
;	c[EDX] = Offset of first fragment for packet
;	c[ESI] = Offset of pointer to first fragment

10$:	PUSHL	ESI			;Save offset of pointer to first
					;  fragment for packet
	MOVZWL	EAX, npb_fragofs.B[EBX]
	CMPW	AX, npb_fragofs.B[EDX]	;Will this be the new first fragment?
	JA	12$.S			;No
	MOVL	EAX, npb_fragtmr.B[EDX]	;Yes - copy the current timer value
	MOVL	npb_fragtmr.B[EBX], EAX
	MOVL	npb_fragnxt.B[EBX], EDX
	MOVL	EAX, [ESI]
	MOVL	npb_next.B[EBX], EAX
	MOVL	[ESI], EBX
	JMP	22$.S			;Now see if we have a complete packet

;Here if this fragment will not become the new first fragment for the packet.
;  Find where it goes in the list.

12$:	MOVL	ECX, npb_count.B[EBX]
	LEAL	ESI, npb_fragnxt.B[EDX]
14$:	MOVL	EDX, [ESI]
	TESTL	EDX, EDX
	JE	20$.S
	CMPW	AX, npb_fragofs.B[EDX]	;Does it go before this fragment?
	JBE	16$.S			;Yes
	CMPL	ECX, npb_count.B[EDX]	;No - is it completely included in this
					;  fragment?
	JBE	18$.S			;Yes - just quietly discard it!
	LEAL	ESI, npb_fragnxt.B[EDX]
	JMP	14$.S

;Here this fragment has an offset less than or equal to the fragment we are
;  checking.  Either it goes before this fragment, it replaces this fragment,
;  or it should be discarded.

16$:	JB	20$.S			;If it definately goes before
	CMPL	EAX, npb_count.B[EDX]	;Same offset - is this one larger?
	JBE	18$.S			;No - discard this fragment
	MOVL	EAX, npb_fragnxt.B[EDX]	;Yes - use this one instead
	MOVL	npb_fragnxt.B[EBX], EAX
	MOVL	[ESI], EBX
	MOVL	EBX, EDX		;Discard the one we had in the list

;Here to discard this fragment because it is completely contained within a
;  fragment we already have

18$:	POPL	EAX
	POPL	ESI
	CALL	xosnetGiveBufr##
	RET

;Here to insert this fragment into the list before the fragment we are checking

20$:	MOVL	npb_fragnxt.B[EBX], EDX
	MOVL	[ESI], EBX

;Since we have added a fragment to the list, we must check and see if we have
;  a complete packet now.

22$:	POPL	ESI			;Get offset of pointer to first fragment
	MOVL	EBX, [ESI]
	CMPW	npb_fragofs.B[EBX], #0.B ;Is this really the first fragment?
	JNE	26$.S			;No - packet is not complete
	MOVL	ECX, npb_count.B[EBX]	;Get total length of first fragment
24$:	MOVL	EDX, npb_fragnxt.B[EBX]	;Get offset of next fragment
	TESTL	EDX, EDX
	JE	28$.S			;If no more
	MOVZBL	EAX, npb_tpofs.B[EBX]
	NEGL	EAX
	ADDL	EAX, npb_count.B[EBX]	;Are these fragments contiguous?
	CMPW	AX, npb_fragofs.B[EDX]
	JB	26$.S			;No - packet is not complete
	MOVL	EBX, EDX		;Yes - advance to next packet
	MOVZBL	EAX, npb_tpofs.B[EBX]	;Get length of everything after the IP
	NEGL	EAX			;  header
	ADDL	EAX, npb_count.B[EBX]
	ADDL	ECX, ECX
	JMP	24$.S

;Here if packet is not complete - just return quietly

26$:	POPL	ESI
	RET

;Here at end of fragment list if all of the fragments are contiguous.  See if
;  the last fragment is the top fragment

28$:	TESTB	npb_fragofs+2.B[EBX], #01h ;Is this the last fragment?
	JNE	26$.S			;No - packet is not complete yet
					;Yes - fall into fragcomp on next page
.PAGE
;Here with a complete fragmented packet - Now we have to put it back together.
;	c{ECX} = Length of complete packet
;	c{EBX} = Offset of last fragment

fragcomp:
	INCL	pdb_ipcntpktfrag.B[EDI]	;Count the fragmented packet
	MOVL	EDX, [ESI]		;Get offset of first fragment
	MOVL	EAX, npb_next.B[EDX]	;Remove it from the fragmented packet
	MOVL	[ESI], EAX		;  list
	MOVL	ECX, npb_count.B[EBX]	;Get total size of packet
	PUSHL	ECX
	CALL	xosnetGetBufr##		;Get a network buffer for the
	POPL	ECX			;  complete packet
	JC	4$.S			;If error
	MOVL	npb_count.B[EBX], ECX
	MOVL	EAX, npb_lpofs.B[EDX]
	MOVL	npb_lpofs.B[EBX], EAX
	MOVL	ECX, npb_count.B[EDX]	;Get total length of the first fragment
	CLD
	PUSHL	EDI
	MOVL	ESI, EDX
	MOVL	EDI, EBX
2$:	PUSHL	DS
	POPL	ES
	RMOVSB	[EDI], [ESI]
	PUSHL	npb_fragnxt.B[EDX]
	XCHGL	EBX, EDX
	CLRL	EAX
	MOVL	npb_fragnxt.B[EBX], EAX
	CALL	xosnetGiveBufr##
	MOVL	EBX, EDX
	POPL	EDX
	TESTL	EDX, EDX
	JE	10$.S
	MOVZBL	ECX, npb_tpofs.B[EDX]
	LEAL	ESI, [EDX+ECX]
	NEGL	ECX
	ADDL	ECX, npb_count.B[EDX]
	MOVZWL	EDI, npb_fragofs.B[EDX]
	SUBL	ECX, EDI
	MOVZBL	EAX, npb_tpofs.B[EBX]
	ADDL	EDI, EAX
	ADDL	EDI, EBX
	JMP	2$.S

;Here if error allocating a buffer for the reconstructed packet - This probably
;  means the system is in big trouble, but we try to handle it as best as we
;  can by simply discarding all of the collected fragments.

4$:	POPL	ESI
	MOVL	EBX, EDX
6$:	PUSHL	npb_fragnxt.B[EBX]	;Give up the buffers
	CALL	xosnetGiveBufr##
	POPL	EBX
	TESTL	EBX, EBX
	JNE	6$.S
	RET

;Here with the reconstructed packet

10$:	MOVL	ECX, EDI
	POPL	EDI
	POPL	ESI
	SUBL	ECX, EBX		;Get length of the packet
	MOVZBL	EAX, npb_tpofs.B[EBX]	;Get length of IP part
	SUBL	ECX, EAX
	MOVB	AL, npb_npofs.B[EBX]	;Get offset of start of IP header
	LEAL	EDX, [EBX+EAX]
					;Fall into rcvrdy on next page
.PAGE
rcvrdy:	MOVL	EAX, ip_dstipaddr.B[EDX] ;Get destination IP address
	CMPL	EAX, #0FFFFFFFFh.B	;Broadcast message?
	JE	12$.S			;Yes - OK
	TESTL	EAX, EAX		;No - all 0?
	JE	12$.S			;Yes - thats OK too
	CMPB	pdb_ipaddr+3.B[EDI], #0	;No - last byte or our IP address = 0?
	JNE	2$.S			;No
	PUSHL	EAX
	ANDL	EAX, #0FFFFFFh		;Yes - ignore the last 8 bits
	CMPL	EAX, pdb_ipaddr.B[EDI]	;For us?
	POPL	EAX
	JMP	4$.S

2$:	CMPL	EAX, pdb_ipaddr.B[EDI]	;For us?
4$:	JE	12$.S			;Yes
	PUSHL	ECX			;Probably not
	PUSHL	EDX
	MOVL	ECX, EAX		;For our net?
	XORL	ECX, pdb_ipaddr.B[EDI]
	ANDL	ECX, pdb_ipnetmask.B[EDI]
	JNE	6$.S			;No
	MOVL	ECX, pdb_ipnetmask.B[EDI] ;Yes - net broadcast?
	NOTL	ECX
	MOVL	EDX, EAX
	ANDL	EDX, ECX
	CMPL	EDX, ECX
	JE	10$.S			;Yes - accept the packet
	MOVL	ECX, EAX		;No - for our subnet?
	XORL	ECX, pdb_ipaddr.B[EDI]
	ANDL	ECX, pdb_ipsubnetmask.B[EDI]
	JNE	6$.S			;No
	MOVL	ECX, pdb_ipsubnetmask.B[EDI] ;Yes - sub-net broadcast?
	NOTL	ECX
	MOVL	EDX, EAX
	ANDL	EDX, ECX
	CMPL	EDX, ECX
	JE	10$.S			;Yes - accept the packet
6$:	POPL	EDX			;No - this packet is not for us!
	POPL	ECX
	INCL	pdb_ipcntbadaddr.B[EDI]
	JMP	ignore

;Here with a packet which is for us

10$:	POPL	EDX
	POPL	ECX
12$:	MOVL	npb_count.B[EBX], ECX
	INCL	pdb_ipcntpktin.B[EDI]	;Count the packet
	ADDL	pdb_ipcntbytein.B[EDI], ECX
	PUSHL	EBX
	PUSHL	EDX
	MOVL	EAX, ip_srcipaddr.B[EDX] ;Get source IP address
	CALL	rtgood#			;Indicate have good route
	POPL	EDX
	POPL	EBX
	MOVB	AL, ip_ipprot.B[EDX]	;Get protocol value
	MOVL	ESI, pdb_iptpdbhead.B[EDI] ;Get first TPDB
14$:	CMPB	AL, tpdb_ipprot.B[ESI]	;This protocol?
	JE	16$.S			;Yes
	MOVL	ESI, [ESI]		;No - advance to next
	TESTL	ESI, ESI		;Have another
	JNE	14$.S			;Yes - continue
	MOVL	EAX, #0203h		;No - get ICMP type and code
	CALL	xosipsSendDstUnr#
	INCL	pdb_ipcntnodst.B[EDI]	;No - count this as "no destination"
ignore:	CALL	xosnetGiveBufr##
	RET

;Here with match on the IP protocol type - dispatch to the receive routine
;  for the transport level protocol
;	c{EAX} = Offset of IDB
;	c{EBX} = Offset of packet buffer
;	c{ECX} = Length of transport level data in packet
;	c{EDX} = Offset of start of transport level data in packet
;	c{EDI} = Offset of PDB
;	c{ESI} = Offset of TPDB

16$:	MOVL	ECX, npb_count.B[EBX]
	MOVZBL	EDX, npb_tpofs.B[EBX]	;Get offset of start of IP data
	ADDL	EDX, EBX
	MOVL	EAX, pdb_idb.B[EDI]	;Get offset of the IDB
	JMPIL	tpdb_rcvdisp.B[ESI]	;Dispatch to routine for protocol

;Here if the link is down
;	c{EAX} = Offset of IDB
;	c{EBX} = Offset of received packet

xosipsLinkDown:
	INCL	idb_cntdown.B[EAX]	;No - count this
	JMP	ignore.S		;And discard the packet
.PAGE
	.SBTTL	iarpprcv - Subroutine to process received IARP packet

;Subroutine to process received IARP packet - note that this version only
;  supports responses - inquiry packets are ignored!
;	c{EBX} = Offset of packet buffer
;	c{ECX} = IARP length
;	c{EDX} = Offset of start of IARP header
;	c{EDI} = Offset of PDB
;	c{ESI} = Offset of IDB
;	CALL	iarprcv

iarprcv:CMPL	ECX, #6.B		;Is it the right length for a response?
	JL	10$.S			;No - ignore it
	MOVZBL	ECX, npb_npofs.B[EBX]	;Yes - point to the IP header
	MOVL	EAX, ip_srcipaddr.B[EBX+ECX] ;Get sender IP address
	MOVL	ECX, [EDX]		;Get remote hardware address
	MOVZWL	EDX, 4.B[EDX]
	PUSHL	EBX
	MOVB	BL, #RTT_LOCAL		;Routing table entry type
	MOVB	BH, #1			;Force update
	CALL	rtupdate#
	POPL	EBX
	JC	8$.S			;Forget it if error here
	MOVL	rt_t2lcnt.B[ECX], #RTT2L_LOCAL ;Store time-to-live value
	MOVL	rt_timeout.B[ECX], #rttimeout# ;Store offset of timeout routine
8$:	JMP	ignore			;Finished here

;Here if the length is wrong - just count this and ignore the packet

10$:	INCL	pdb_ipcntpsltmn.B[EDI]
	JMP	8$.S
.PAGE
	.SBTTL	ipsprocarp - Subroutine to process ARP packets

;Subroutine to process ARP packets - called with an parp_ stack frame set up
;	CALL	ipsprocarp
;	parp_oper is set to 0 to suppress sending a response

ipsprocarp:
	MOVL	EAX, parp_tpa.B[EBP]	;Get target IP address
	CMPL	pdb_ipaddr.B[EDI], EAX	;Is it for us?
	JNE	8$.S			;No
6$:	MOVB	BH, #1			;Yes - force routing table update
	JMP	10$.S

;Here if message is not for us - make it look like a reply so we don't respond
;  to it

8$:	MOVB	parp_oper.B[EBP], #02
	MOVB	BH, #0			;Do not force the update
10$:	MOVB	BL, #RTT_LOCAL		;Get routing table entry type
	MOVL	EAX, parp_spa.B[EBP]	;Get IP address
	MOVL	ECX, parp_sha+0.B[EBP]	;Get hardware address
	MOVL	EDX, parp_sha+4.B[EBP]
	CALL	rtupdate#		;Update the routing table
	JC	14$.S			;If error
	JREGZ	ECX, 12$		;If no routing table entry
	MOVL	rt_t2lcnt.B[ECX], #RTT2L_LOCAL ;Store time-to-live value
	MOVL	rt_timeout.B[ECX], #rttimeout# ;Store offset of timeout routine
12$:	RET

14$:	MOVB	parp_oper.B[EBP], #0	;Indicate no response
	CLC
	RET
.PAGE
	.SBTTL	xosipsSetPkt - Subroutine to set up IP datagram packet header

;Subroutine to set up IP datagram packet header - must be called at in extended
;  fork context
;	c{EDX} = IP protocol in bits 7-15
;	c{EBX} = Offset of packet buffer
;	c{EDI} = Offset of DCB
;	CALL	xosipsSetPkt
;	C:set = Error
;	  c{EAX} = Error code
;	  c{EBX} = Offset of packet buffer (unchanged)
;	C:clr = Normal
;	  c{EBX} = Offset of packet buffer (unchanged)
;	  c{EDX} = Offset of first byte beyond IP header

xosipsSetPkt:
	MOVL	EAX, dcb_ipsrmtaddr.B[EDI] ;Get IP address
	PUSHL	EDI
	MOVL	EDI, dcb_netpdb.B[EDI]	;Get offset of PDB
	CALL	xosipsSetPkt2
	POPL	EDI
	RET

;Subroutine to set up IP packet - does not require DCB or XFF so can be called
;  from device fork level;  
;	c{EAX} = IP address
;	c{EDX} = IP protocol in bits 7-15
;	c{EDI} = Offset of PDB
;	CALL	xosipsSetPkt2
;	C:set = Error
;	  c{EAX} = Error code
;	  c{EBX} = Offset of packet buffer (unchanged)
;	C:clr = Normal
;	  c{EBX} = Offset of packet buffer (unchanged)
;	  c{EDX} = Offset of first byte beyond IP header

xosipsSetPkt2:
	CMPL	pdb_magic.B[EDI], #'PDB*'
	JNE	10$.S
	PUSHL	EAX			;Save IP address
	PUSHL	EDX			;Save IP protocol
	MOVL	EAX, pdb_etypevalue.B[EDI] ;Get hardware protocol value
	MOVL	ESI, pdb_sdb.B[EDI]
	CALL	xossnpSetPkt##		;Set up SNAP level header
	JC	8$.S
	MOVL	ip_vershlen.B[EDX], #45h ;Store version, header length, and
	CLRL	EAX			 ;  type of service
	MOVL	ip_ident.B[EDX], EAX	;Store ident and fragment data (both 0)
	POPL	EAX
	MOVB	AL, #32t		;Get time to live value to use
	MOVL	ip_time.B[EDX], EAX	;Store time to live, IP protocol, and
					;  header checksum value (0)
	MOVL	EAX, pdb_ipaddr.B[EDI]	;Store source IP address
	MOVL	ip_srcipaddr.B[EDX], EAX
	POPL	EAX			;Store destination IP address
	MOVL	ip_dstipaddr.B[EDX], EAX
ipsetpkt4:
	ADDL	npb_count.B[EBX], #ip_options.B ;Increase packet byte count
	ADDL	EDX, #ip_options.B	;Point to first byte beyond the header
	MOVB	AL, npb_npofs.B[EBX]	;Store offset of next level header
	ADDB	AL, #ip_options
	MOVB	npb_tpofs.B[EBX], AL
	RET				;Return

;Here if error

8$:	POPL	EDX			;Fix up the stack
	POPL	EDX
ret008:	RET

10$:	CRASH	NPDB			;[Not a PDB]
.PAGE
	.SBTTL	xosipsSetEcho - Subroutine to set up IP echo packet header

;Subroutine to set up IP echo packet header - must be called in extended fork
;  context or at device fork level
;	c{EBX} = Offset of packet buffer
;	c{EDX} = Offset of start of IP header in packet
;	c{EDI} = Offset of PDB
;	CALL	xosipsSetEcho
;	C:set = Error
;	  c{EAX} = Error code
;	C:clr = Normal
;	  c{EBX} = Offset of packet buffer (unchanged)
;	  c{EDX} = Offset of first byte beyond IP header
;  Note that this subroutine resets npb_count to include only the hardware
;    and IP headers.  If data beyond the IP header is to be echoed, npb_count
;    must be increased after ipsetecho returns

xosipsSetEcho:
	CMPL	pdb_magic.B[EDI], #'PDB*'
	JNE	4$.S
	MOVL	EAX, pdb_etypevalue.B[EDI] ;Get hardware protocol value
	MOVL	ESI, pdb_sdb.B[EDI]
	CALL	xossnpSetPkt##		;Set up SNAP level header
	JC	ret008.S
	MOVW	ip_vershlen.B[EDX], #45h ;Store version, header length, and
	CLRL	EAX			 ;  type of service
	MOVL	ip_ident.B[EDX], EAX	;Store ident and fragment data (both 0)
	MOVW	ip_hdrchksum.B[EDX], AX	;Clear header checksum value
	MOVB	ip_time.B[EDX], #32t	;Store time to live
	MOVL	EAX, ip_srcipaddr.B[EDX] ;Store source IP address as the
	MOVL	ip_dstipaddr.B[EDX], EAX ;  destination address
	MOVL	EAX, pdb_ipaddr.B[EDI]	 ;Store or IP address as the source
	MOVL	ip_srcipaddr.B[EDX], EAX ;  address
	JMP	ipsetpkt4.S

4$:	CRASH	NPDB			;[Not a PDB]
.PAGE
	.SBTTL	xosipsSendSetup - Subroutine to finish setting up IP header for output

;Subroutine to finish setting up the IP packet header before outputting a
;  packet
;	c{EBX} = Offset of packet buffer
;	c{EDI} = Offset of DCB
;	CALL	xosipsSendSetup

xosipsSendSetup:
	PUSHL	EDI
	MOVL	EDI, dcb_netpdb.B[EDI]	;Get offset of protocol data block
	CALL	xosipsSendSet2
	POPL	EDI
	RET

xosipsSendSet2:
	MOVL	EAX, npb_count.B[EBX]	;Get total length of packet
	MOVZBL	ECX, npb_npofs.B[EBX]	;Get offset of start of IP part
	SUBL	EAX, ECX		;Calculate length of IP part
	INCL	pdb_ipcntpktout.B[EDI]	;Bump our counts
	ADDL	pdb_ipcntbyteout.B[EDI], EAX
	PUSHL	EBX
	ADDL	EBX, ECX
	XCHGB	AL, AH			;Swap bytes
	MOVW	npb_data+ip_tlength.B[EBX], AX ;Store in IP header
	MOVW	npb_data+ip_hdrchksum.B[EBX], #0 ;Clear the check-sum field
	TESTB	pdb_ipchksumh[EDI], #PCSH$GEN ;Should we calculate the checksum
					      ;  value?
	JE	10$.S			;No - finished here
	PUSHL	ESI			;Yes
	LEAL	ESI, npb_data+16t.B[EBX]
	CLD
	LODSL	[ESI]			;Calculate checksum for first 5 longs
	ADDL	EAX, -20t.B[ESI]
	ADCL	EAX, #0.B
	ADDL	EAX, -16t.B[ESI]
	ADCL	EAX, #0.B
	ADDL	EAX, -12t.B[ESI]
	ADCL	EAX, #0.B
	ADDL	EAX, -8.B[ESI]
	ADCL	EAX, #0.B
	MOVB	CL, npb_data+ip_vershlen.B[EBX]
	ANDL	ECX, #0Fh.B		;Get number of longs in header
	SUBL	ECX, #5.B		;Any more to do?
	JLE	6$.S			;No
4$:	ADDL	EAX, [ESI]		;Yes
	ADCL	EAX, #0.B
	ADDL	ESI, #4.B
	LOOP	ECX, 4$
6$:	POPL	ESI
	MOVL	EDX, EAX		;Merge halves
	SHRL	EDX, #16t
	ADDW	AX, DX
	ADCW	AX, #0.B
	NOTL	EAX
	MOVW	npb_data+ip_hdrchksum.B[EBX], AX
10$:	POPL	EBX			;Restore offset of the packet buffer
	RET
.PAGE
;Subroutine to add in final n bytes of a packet to the transprot level checksum
;  value.  This routine implements the standard Internet 16 bit 1's complement
;  checksum xosorithm.
;	c{EAX} = Accumulated 32 bit checksum value
;	c{ECX} = Number of bytes remaining
;	c{ESI} = Offset of next byte in packet
;	CALL	xosipsChkSumFin
;	c[AX] = Final 16 bit checksum value
;  All register except EAX and ESI are preserved

xosipsChkSumFin::
	PUSHL	ECX			;Save initial count
	SHRL	ECX, #2			;Change to number of longs (round down)
	JE	4$.S			;If no whole longs
2$:	ADDL	EAX, [ESI]		;Add in next 4 bytes
	ADCL	EAX, #0.B
	ADDL	ESI, #4.B		;Bump pointer
	LOOP	ECX, 2$			;Continue if more
4$:	MOVL	ECX, [ESP]		;Restore count
	ANDL	ECX, #03h.B		;Get low 2 bits only
	JMPIL	cksfdsp[ECX*4]		;Dispatch on number of bytes left

	DATA

	.MOD	4
cksfdsp:.LONG	cksfb0		;0 bytes left
	.LONG	cksfb1		;1 byte left
	.LONG	cksfb2		;2 bytes left
	.LONG	cksfb3		;3 bytes left

	CODE

;Here if have 1 byte left

cksfb1:	MOVZBL	ECX, [ESI]		;Get one byte
	JMP	6$.S

;Here if have 2 bytes left

	.MOD	4
cksfb2:	MOVZWL	ECX, [ESI]		;Get two bytes
	JMP	6$.S

;Here if have 3 bytes left

	.MOD	4
cksfb3:	MOVZWL	ECX, 1.B[ESI]		;Get second and third bytes
	SHLL	ECX, #8
	MOVB	CL, [ESI]		;Get first byte
6$:	ADDL	EAX, ECX		;Add into checksum value
	ADCL	EAX, #0.B
cksfb0:	MOVL	ECX, EAX		;Add the two halves together
	SHRL	ECX, #16t
	ADDW	AX, CX			;(must be ADDW to get C right!)
	ADCL	EAX, #0.B
	POPL	ECX
	RET
.PAGE
	.SBTTL	ipsoas - IPS once-a-second routine

;This routine times out collection of fragmented packets.

ipsoas:	MOVL	EDI, xosipsPdbHead
	TESTL	EDI, EDI
	JE	16$.S
4$:	TOFORK
	LEAL	ESI, pdb_ipfraghead[EDI]
	MOVL	EBX, [ESI]		;Get first packet
	TESTL	EBX, EBX
	JE	14$.S			;If none
6$:	DECL	npb_fragtmr.B[EBX]	;Reduce the timer count
	JNS	10$.S			;Go on if still some time left
	MOVL	EAX, npb_next.B[EBX]	;Timed out - remove from the fragmented
	MOVL	[ESI], EAX		;  packet list
8$:	INCL	pdb_ipcntdisfrag.B[EDI]	;Count the discarded fragment
	PUSHL	npb_fragnxt.B[EBX]	;Save offset of next fragment buffer
	MOVL	npb_fragnxt.B[EBX], #0	;Must clear this here so xosnetGiveBufr
					;  does not get confussed!
	CALL	xosnetGiveBufr##	;Give up the fragment buffer
	POPL	EBX
	TESTL	EBX, EBX		;Have another fragment buffer?
	JNE	8$.S			;Yes - continue
	JMP	12$.S			;No

;Here if this partial packet has not timed out yet

10$:	LEAL	ESI, npb_next.B[EBX]	;Advance to next packet in list
12$:	MOVL	EBX, [ESI]
	TESTL	EBX, EBX
	JNE	6$.S			;Continue if more partial packets
14$:	FROMFORK			;No more for this PDB
	MOVL	EDI, pdb_nextd.B[EDI]	;Advance to next PDB
	TESTL	EDI, EDI
	JNE	4$.S			;Continue if more PDBs
16$:	JMP	routeoas#		;Finished here - go do routing stuff
.PAGE
	.SBTTL	Data

	DATA

lit10::       .LONG 10t
ipsccb:       .BLKB ccb_SIZE	;IPS CCB
icmpccb::     .BLKB ccb_SIZE	;ICMP CCB
xosipsPdbHead:.LONG 0
temp:	      .BLKB 12t		;Buffer for default address characteristics
				;  names
firstndb::    .LONG 0		;Offset of first NDB
lastndb::     .LONG 0		;Offset of last NDB
numndb::      .LONG 0		;Number of NDBs

	LKEEND
